<style type="text/css">
  .btn {
  background-color: #f4511e;
  border: none;
  color: white;
  padding: 16px 32px;
  text-align: center;
  font-size: 16px;
  margin: 4px 2px;
  opacity: 0.6;
  transition: 0.3s;
}
.btn:hover {opacity: 1}
textarea {
  height:50%;
}


</style>
<body>

  <nav class="navbar navbar-dark bg-dark">
      <div class="container-fluid">
        <div class="navbar-header">
            <a class="navbar-brand" href="/home">unITed</a>
        </div>
        <ul class="nav navbar-nav navbar-right">
            <li><a href="/logout">Logout</a></li>
        </ul>
      </div>
  </nav>
  <br><br>
  <form action="/signup_user" method="post">
  <div class="container">
    <div class="form-group">
    <label for="usr">Rollno</label>
    <input type="text" class="form-control" id="rollno" name="rollno" value="<%= rollno %>" readonly="true">
    </div>
    <div class="form-group">
    <label for="pwd">Password:</label>
    <input type="password" class="form-control" id="password" name="password">
    </div>
    <label for="foodpref">Food Prefrence</label>
    <div class="radio">
      <label><input type="radio" name="foodpref" value="veg" checked>Veg</label>
    </div>
    <div class="radio">
      <label><input type="radio" name="foodpref" value="nonveg">Non-veg</label>
    </div>
    <label for="foodpref">Do you have memories to share with us?</label>
    <div class="radio">
      <label><input type="radio" name="memories" value="yes" checked>Yes</label>
    </div>
    <div class="radio">
      <label><input type="radio" name="memories" value="no">No</label>
    </div>
    <div class="form-group">
    <label for="childhoodpic">Childhood Pic</label>
    <input type="file" class="form-control" id="childpic" name="childpic">
    </div>
    <input type="submit">
    
  </div>
</form>
</body>
</html>
